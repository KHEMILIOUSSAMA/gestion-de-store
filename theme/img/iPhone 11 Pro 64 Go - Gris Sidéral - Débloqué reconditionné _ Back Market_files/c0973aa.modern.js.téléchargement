.label"\n        :for="id"\n        tag="label"\n        :variant="labelVariant"\n      >\n        {{ label }}\n      </Typography>\n\n      <Typography\n        :class="$style.selectBase"\n        tag="div"\n        :variant="selectBaseVariant"\n      >\n        <select\n          :id="id"\n          v-bind="$attrs"\n          :aria-labelledby="optionLabelId"\n          :class="$style.select"\n          :disabled="disabled"\n          :value="selectedValue"\n          v-on="listeners"\n        >\n          <option\n            v-if="isTiny"\n            :id="optionLabelId"\n            disabled\n            hidden\n            selected\n            value=""\n          >\n            {{ label }}\n          </option>\n\n          <optgroup\n            v-for="group in groupsToDisplay"\n            :key="group.key"\n            :data-testid="group.label"\n            :disabled="group.disabled"\n            :label="group.label"\n          >\n            <option\n              v-for="option in group.options"\n              :key="option.key"\n              :disabled="option.disabled"\n              :value="option.key"\n            >\n              {{ option.label }}\n            </option>\n          </optgroup>\n\n          <option\n            v-for="option in optionsToDisplay"\n            :key="option.key"\n            :disabled="option.disabled"\n            :value="option.key"\n          >\n            {{ option.label }}\n          </option>\n        </select>\n\n        <IconChevronDown :class="$style.chevronDown" />\n      </Typography>\n    </div>\n\n    <Typography\n      v-if="shouldDisplayHelperText"\n      :class="$style.helperText"\n      tag="p"\n      :variant="TypographyVariants.Body2Light"\n    >\n      {{ helperText }}\n    </Typography>\n  </div>\n</template>\n\n<script lang="ts">\nimport isEmpty from \'lodash.isempty\'\nimport { Component, Prop, Vue } from \'vue-property-decorator\'\n\nimport Typography, {\n  Variants as TypographyVariants,\n} from \'