of the select.\n   */\n  @Prop({ required: true, type: [Array, Object, String] })\n  readonly value!: Value\n\n  /**\n   * The variant to display the select in.\n   *\n   * Allowed values are:\n   * ```\n   * \'default\' | \'defaultInverted\' | \'tiny\' | \'tinyInverted\'\n   * ```\n   */\n  @Prop({ required: true, type: String })\n  readonly variant!: Variants\n\n  private readonly TypographyVariants = TypographyVariants\n\n  get allDisplayedOption() {\n    return [\n      ...this.optionsToDisplay,\n      ...this.groupsToDisplay.reduce(\n        (allOptions: Array<DisplayableOption>, { options }) => [\n          ...allOptions,\n          ...options,\n        ],\n        [],\n      ),\n    ]\n  }\n\n  get classNames() {\n    const classNames: Array<string | { [key: string]: boolean }> = [\n      {\n        [this.$style.disabled]: this.disabled,\n        [this.$style.error]: this.hasError,\n        [this.$style.labelOnTop]: this.isLabelOnTop,\n      },\n    ]\n\n    switch (this.variant) {\n      case Variants.Default:\n        classNames.push(this.$style.defaultVariant)\n        break\n\n      default:\n        classNames.push(this.$style[this.variant])\n        break\n    }\n\n    return classNames\n  }\n\n  get labelVariant() {\n    return this.isLabelOnTop\n      ? TypographyVariants.Body3Light\n      : TypographyVariants.Body2Light\n  }\n\n  get groupsToDisplay() {\n    if (isEmpty(this.options)) {\n      return []\n    }\n\n    return this.options\n      .filter((option) => {\n        const { group: isGroup, options } = option as OptionsGroup\n\n        return isGroup && !isEmpty(options)\n      })\n      .reduce(\n        (groups: Array<DisplayableGroup>, optionGroup: Option, groupIndex) => {\n          const group = optionGroup as OptionsGroup\n          const key = `${group.label}-${groupIndex}`\n\n          return [\n            ...groups,\n            {\n              disabled: group.disabled,\n              key,\n              label: group.label,\n              options: group.options.map((option, optionIndex) =>\n                this.transformOption(option, optionIndex, key),\n              ),\n            },\n          ]\n        },\n        [],\n      )\n  }\n\n  get hasError() {\n    return !isEmpty(this.error)\n  }\n\n  get hasHelperText() {\n    return !isEmpty(this.helperText)\n  }\n\n  get hasValue() {\n    return !isEmpty(this.value)\n  }\n\n  get helperText() {\n    return this.hasError ? this.error : this.description\n  }\n\n  get isLabelOnTop() {\n    return !this.isTiny && this.hasValue\n  }\n\n  get isTiny() {\n    return (\n      this.variant === Variants.Tiny || this.variant === Variants.TinyInverted\n    )\n  }\n\n  get listeners() {\n    return {\n      ...this.$listeners,\n      change: this.onChange,\n    }\n  }\n\n  get optionLabelId() {\n    return this.isTiny ? `${this.id}-label-option` : null\n  }\n\n  get optionsToDisplay() {\n    if (isEmpty(this.options)) {\n      return []\n    }\n\n    return this.options\n      .filter((option) => !(option as OptionsGroup).group)\n      .reduce(\n        (options: Array<DisplayableOption>, option: Option, optionIndex) => {\n          return [...options, this.transformOption(option, optionIndex)]\n        },\n        [],\n      )\n  }\n\n  get selectBaseVariant() {\n    return this.isTiny\n      ? TypographyVariants.Body2Bold\n      : TypographyVariants.Body2Light\n  }\n\n  get shouldDisplayHelperText() {\n    return !this.isTiny && this.hasHelperText\n  }\n\n  get selectedValue() {\n    const matchingOption = this.allDisplayedOption.find(\n      ({ value }) => this.value === value,\n    )\n\n    if (matchingOption !== undefined && !isEmpty(matchingOption)) {\n      return matchingOption.key\n    }\n\n    return null\n  }\n\n  onChange(event: Event & { target: HTMLSelectElement }) {\n    this.$emit(\'change\', event)\n\n    const matchingOption = this.allDisplayedOption.find(\n      ({ key }) => key === event.target.value,\n    )\n    if (matchingOption !== undefined && !isEmpty(matchingOption)) {\n      this.$emit(MODEL_EVENT, matchingOption.value)\n    }\n  }\n\n  transformOption(\n    option: Option,\n    index: number,\n    prefix = \'\',\n  ): DisplayableOption {\n    const standardizedOption: OptionItem =\n      typeof option === \'string\'\n        ? {\n            disabled: false,\n            label: option,\n            value: option,\n          }\n        : (option as OptionItem)\n\n    const keyPrefix = isEmpty(prefix) ? \'\' : `${prefix}/`\n\n    return {\n      key: `${keyPrefix}${standardizedOption.label}-${index}`,\n      ...standardizedOption,\n    }\n  }\n}\n<\/script>\n\n<style lang="postcss" module>\n.selectWrapper {\n  position: relative;\n  height: 48px;\n\n  border-width: 1px;\n  border-style: solid;\n  border-radius: var(--border-radius-2);\n}\n\n.label {\n  display: flex;\n\n  position: absolute;\n  top: 0;\n  left: var(--spacing-3);\n  right: var(--spacing-3);\n\n  height: 100%;\n  align-items: center;\n\n  /* In case of a lengthy label, hide the rest of the label under an ellipsis. */\n  max-width: 100%;\n  /* Make sure to stop before the select dropdown down chevron. */\n  padding-right: var(--spacing-4);\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n\n  /* Disable interactions with the label. */\n  user-select: none;\n  pointer-events: none;\n\n  background-color: inherit;\n}\n\n.selectBase {\n  height: 100%;\n  width: 100%;\n\n  display: flex;\n  align-items: center;\n\n  border-radius: var(--border-radius-2);\n\n  background-color: inherit;\n}\n\n.select {\n  height: 100%;\n  width: 100%;\n  padding: var(--spacing-5) 0 0 var(--spacing-3);\n\n  border: none;\n  border-radius: var(--border-radius-2);\n\n  font: inherit;\n  color: inherit;\n  background-color: inherit;\n  outline: none;\n\n  -webkit-appearance: none;\n  -moz-appearance: none;\n}\n\n/*\n For unknown reasons, the Select in Firefox has an "internal" padding of 2px\n which causes a misalignment with the label. Providing a negative text indent\n while targetting only Firefox is one viable solution.\n @see https://stackoverflow.com/questions/952861/targeting-only-firefox-with-css\n @see https://imperatorz.slack.com/archives/G8C7PK073/p1612368846009600\n*/\n@-moz-document url-prefix() {\n  .select {\n    text-indent: -2px;\n  }\n}\n\n.chevronDown {\n  position: absolute;\n  right: var(--spacing-3);\n\n  /* Center the chevron in the middle of it\'s container. */\n  top: 50%;\n  transform: translateY(-50%);\n\n  /* Disable interactions with the chevron. */\n  user-select: none;\n  pointer-events: none;\n}\n\n.helperText {\n  margin-top: var(--spacing-3);\n}\n\n/* LABEL ON TOP & FOCUS STATE */\n.labelOnTop .label {\n  display: unset;\n\n  height: auto;\n  top: var(--spacing-2);\n  transform: unset;\n\n  background-color: transparent;\n}\n\n/* VARIANT DEFAULT AND TINY DEFAULT */\n.defaultVariant .selectWrapper,\n.tiny .selectWrapper {\n  border-color: var(--grey-300);\n  background-color: var(--white);\n}\n\n.defaultVariant .label,\n.defaultVariant .select,\n.tiny .select {\n  color: var(--black);\n}\n\n.defaultVariant .helperText,\n.defaultVariant.labelOnTop .label {\n  color: var(--grey-500);\n}\n\n.defaultVariant .chevronDown,\n.tiny .chevronDown {\n  color: var(--grey-500);\n}\n\n/* FOCUS STATE */\n.defaultVariant .selectWrapper:focus-within {\n  border-color: var(--grey-500);\n}\n\n/* DISABLED STATE */\n.defaultVariant.disabled .selectWrapper,\n.tiny.disabled .selectWrapper {\n  background-color: var(--grey-200);\n}\n.defaultVariant.disabled .label,\n.defaultVariant.disabled .select,\n.tiny.disabled .select {\n  color: var(--grey-500);\n}\n.defaultVariant.disabled.labelOnTop .label,\n.defaultVariant.disabled .helperText {\n  color: var(--grey-400);\n}\n\n/* ERROR STATE */\n.defaultVariant.error .selectWrapper,\n.tiny.error .selectWrapper {\n  border-color: var(--red-500);\n}\n.defaultVariant.error .helperText {\n  color: var(--red-700);\n}\n\n/* VARIANT DEFAULT INVERTED */\n.defaultInverted .selectWrapper,\n.tinyInverted .selectWrapper {\n  border-color: var(--grey-600);\n  background-color: var(--black);\n}\n\n.defaultInverted .label,\n.defaultInverted .select,\n.tinyInverted .select {\n  color: var(--grey-100);\n}\n\n.defaultInverted .helperText,\n.defaultInverted.labelOnTop .label {\n  color: var(--grey-400);\n}\n\n.defaultInverted .chevronDown {\n  color: var(--grey-500);\n}\n\n/* FOCUS STATE */\n.defaultInverted .selectWrapper:focus-within {\n  border-color: var(--grey-500);\n}\n\n/* DISABLED STATE */\n.defaultInverted.disabled .selectWrapper,\n.tinyInverted.disabled .selectWrapper {\n  background-color: var(--grey-700);\n}\n.defaultInverted.disabled .label,\n.defaultInverted.disabled .select,\n.tinyInverted.disabled .select {\n  color: var(--grey-400);\n}\n.defaultInverted.disabled.labelOnTop .label {\n  color: var(--grey-500);\n}\n.defaultInverted.disabled .helperText {\n  color: var(--grey-400);\n}\n\n/* ERROR STATE */\n.defaultInverted.error .selectWrapper,\n.tinyInverted.error .selectWrapper {\n  border-color: var(--red-500);\n}\n.defaultInverted.error .helperText {\n  color: var(--red-500);\n}\n\n/* TINY VARIANT */\n.tiny .selectWrapper,\n.tinyInverted .selectWrapper {\n  display: inline-block;\n  height: 32px;\n\n  border-color: transparent;\n}\n.tiny .selectWrapper {\n  background-color: var(--grey-200);\n}\n.tinyInverted .selectWrapper {\n  background-color: var(--grey-700);\n}\n\n.tiny .select,\n.tinyInverted .select {\n  height: 100%;\n  margin-top: 0;\n  /* margin-right: var(--spacing-7); */\n  padding: 0 calc(var(--spacing-7) + var(--spacing-3)) 0 var(--spacing-3);\n}\n.tiny .select {\n  color: var(--black);\n}\n.tinyInverted .select,\n.tinyInverted .chevronDown {\n  color: var(--grey-100);\n}\n\n.tiny.error .selectWrapper,\n.tinyInverted.error .selectWrapper {\n  border-color: var(--red-500);\n}\n\n@media (prefers-reduced-motion: no-preference) {\n  .selectWrapper {\n    transition: border-color var(--tr