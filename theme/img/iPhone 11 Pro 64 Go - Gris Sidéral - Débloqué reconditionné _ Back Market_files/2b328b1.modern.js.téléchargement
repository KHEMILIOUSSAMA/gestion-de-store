rker(index: number, eventType: string) {\n    try {\n      if (eventType === Events.Keyboard) {\n        this.markers[index].focus()\n      } else {\n        this.markers[index].dispatchEvent(new Event(\'mousedown\'))\n        this.markers[index].dispatchEvent(new TouchEvent(\'touchstart\'))\n      }\n    } catch {\n      // TouchEvent constructor may not be defined.\n      // See: https://caniuse.com/#feat=mdn-api_touchevent_touchevent\n    }\n  }\n\n  updateValue(value: Array<number>) {\n    this.$emit(\'change\', value)\n    this.$emit(MODEL_EVENT, value)\n  }\n}\n<\/script>\n\n<style lang="postcss" module>\n.rangeWrapper {\n  padding: 0 var(--spacing-4);\n\n  @media (--large) {\n    padding: 0 var(--spacing-3);\n  }\n}\n\n.container {\n  position: relative;\n  height: 32px;\n  display: flex;\n  align-items: center;\n\n  @media (--large) {\n    height: 24px;\n  }\n}\n\n.line {\n  width: 100%;\n  height: 4px;\n  border-radius: var(--border-radius-small);\n\n  @media (--large) {\n    height: 2px;\n  }\n}\n\n.container:not(.disabled) .line {\n  cursor: pointer;\n}\n\n.defaultVariant .line {\n  background: var(--grey-300);\n}\n\n.defaultInverted .line {\n  background: var(--grey-500);\n}\n\n.defaultInverted.disabled .line {\n  background: var(--grey-600);\n}\n\n.innerLine {\n  height: 4px;\n  position: absolute;\n  pointer-events: none;\n\n  @media (--large) {\n    height: 2px;\n  }\n}\n\n.defaultVariant .innerLine {\n  background: var(--black);\n}\n\n.defaultVariant.disabled .innerLine {\n  background: var(--grey-400);\n}\n\n.defaultInverted .innerLine {\n  background: var(--white);\n}\n\n.defaultInverted.disabled .innerLine {\n  background: var(--grey-500);\n}\n\n.marker {\n  z-index: 1;\n  position: absolute;\n  top: 50%;\n  height: 32px;\n  width: 32px;\n  transform: translate(-50%, -50%);\n  border-radius: var(--border-radius-3);\n  outline: none;\n\n  @media (--large) {\n    height: 24px;\n    width: 24px;\n  }\n}\n\n.marker:focus {\n  z-index: 2;\n}\n\n.defaul