d = () => {\n        containerX = 0\n        containerWidth = 0\n\n        document.removeEventListener(\'touchmove\', drag)\n        document.removeEventListener(\'touchend\', dragEnd)\n\n        document.removeEventListener(\'mousemove\', drag)\n        document.removeEventListener(\'mouseup\', dragEnd)\n      }\n\n      const dragStart = (event: MouseEvent | TouchEvent) => {\n        event.preventDefault()\n\n        const { left, width } = this.container.getBoundingClientRect()\n\n        containerX = left\n        containerWidth = width\n\n        if (\'touches\' in event) {\n          document.addEventListener(\'touchmove\', drag)\n          document.addEventListener(\'touchend\', dragEnd)\n        } else {\n          document.addEventListener(\'mousemove\', drag)\n          document.addEventListener(\'mouseup\', dragEnd)\n        }\n\n        this.markers[index].focus()\n      }\n\n      const set = (nextValue: number, eventType: string) => {\n        if (this.disabled) {\n          return\n        }\n\n        // Prevents value to go lower than minimal value.\n        if (nextValue < this.min) {\n          this.updateValue(getUpdatedValue(this.value, index, this.min))\n\n          return\n        }\n\n        // Prevents value to go higher than maximal value.\n        if (nextValue > this.max) {\n          this.updateValue(getUpdatedValue(this.value, index, this.max))\n\n          return\n        }\n\n        // Make sure the first marker can\'t get higher than the second one.\n        if (index === 0 && nextValue > this.value[1]) {\n          dragEnd()\n          this.switchMarker(1, eventType)\n          this.updateValue([this.value[1], Math.round(nextValue)])\n\n          return\n        }\n\n        // Make sure the second marker can\'t get lower than the first one.\n        if (index === 1 && nextValue < this.value[0]) {\n          dragEnd()\n          this.switchMarker(0, eventType)\n          this.updateValue([Math.round(nextValue), this.value[0]])\n\